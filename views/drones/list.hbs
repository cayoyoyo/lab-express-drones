<h2>Available drones</h2>


<div class="drones">
  {{#each drones}}
  <div class="drone">
    <h3>Name: {{this.name}}</h3>
    <p>Propellers: {{this.propellers}}</p>
    <p>Max Speed: {{this.maxSpeed}}</p>
      <a href="/drones/{{_id}}/edit" class="edit-button">Edit</a>
      <form action="/drones/{{_id}}/delete" method="POST">
        <button type="submit" class="delete-button">Delete</button>
      </form>
  </div>
  {{/each}}
</div>
